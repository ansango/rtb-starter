{
  "name": "rtb-starter",
  "license": "MIT",
  "version": "0.0.7",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/ansango/rtb-starter.git"
  },
  "homepage": "https://rtb-starter.vercel.app/",
  "keywords": [
    "react",
    "typescript",
    "tailwindcss",
    "storybook",
    "jest",
    "react-unit-testing",
    "eslint",
    "prettier",
    "rollup",
    "plop",
    "huksy",
    "starter"
  ],
  "peerDependencies": {
    "postcss": "^8.4.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "tailwindcss": "^3.0.16"
  },
  "scripts": {
    "start": "start-storybook -p 6006 -s public",
    "lint": "eslint src/**/*.{js,jsx,ts,tsx}",
    "test": "npm run lint && npm run test:clean && jest --maxWorkers=50%",
    "test:clean": "rimraf coverage",
    "test:watch": "jest --watch --maxWorkers=25%",
    "test:ci": "jest --runInBand",
    "build:dist": "npm run clean:dist && npm run test && rollup -c",
    "clean:dist": "rimraf dist",
    "test:sb": "npm run test:clean && jest --maxWorkers=50%",
    "build:sb": "npm run clean:sb && npm run test:sb && build-storybook -s public",
    "clean:sb": "rimraf storybook-static",
    "create:s": "plop --plopfile generators/sample/generator.js",
    "create:c": "plop --plopfile generators/component/generator.js",
    "clean:all": "rimraf dist && rimraf storybook-static && rimraf coverage",
    "build:sb:clean": "npm run build:sb && npm run clean:all",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@babel/core": "^7.16.12",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@storybook/addon-a11y": "^6.4.15",
    "@storybook/addon-actions": "^6.4.14",
    "@storybook/addon-essentials": "^6.4.14",
    "@storybook/addon-links": "^6.4.14",
    "@storybook/addon-storysource": "^6.4.15",
    "@storybook/addons": "^6.4.15",
    "@storybook/builder-webpack5": "^6.4.14",
    "@storybook/manager-webpack5": "^6.4.14",
    "@storybook/node-logger": "^6.4.14",
    "@storybook/preset-create-react-app": "^4.0.0",
    "@storybook/react": "^6.4.14",
    "@storybook/theming": "^6.4.15",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.12",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "autoprefixer": "^10.4.2",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.28.0",
    "husky": "^7.0.0",
    "jest": "^27.4.7",
    "plop": "^3.0.5",
    "postcss": "^8.4.5",
    "prettier": "^2.5.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "^5.0.0",
    "rollup": "^2.66.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-typescript2": "^0.31.1",
    "tailwindcss": "^3.0.17",
    "typescript": "^4.5.5",
    "webpack": "^5.67.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "files": [
    "/dist"
  ]
}
